{
  "editor.formatOnSave": true,
  "files.exclude": {
    "**/node_modules": true
  },
  "editor.defaultFormatter": "biomejs.biome",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "terminal.integrated.cwd": "${workspaceFolder}/frontend"
}